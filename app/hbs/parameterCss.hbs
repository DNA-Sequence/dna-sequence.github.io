<style id="parameterCss">
        {{#color}}

        body {
            background-color: {{background}};
        }

            {{#row}}
            .row {
                background-color: {{background}};
            }
            {{/row}}

            {{#alignCandidate}}
            .alignCandidate {
                fill: {{fill}};
                stroke: {{stroke}};
                background-color: {{background}};
            }
            {{/alignCandidate}}

            {{#align}}
            .align {
                fill: {{fill}};
                stroke: {{stroke}};
            }
            {{/align}}
            {{#noAlign}}
            .noAlign {
                fill: {{fill}};
                stroke: {{stroke}};
            }
            {{/noAlign}}

            {{#scores}}
            .scores {
                background-color: {{background}};
            }
                {{#p}}
                .scores p {
                    background-color: {{background}};
                }
                {{/p}}

            {{/scores}}

            {{#alignCandidateSelect}}
            .alignCandidateSelect {
                fill: {{fill}};
                stroke: {{stroke}};
                background-color: {{background}};
            }
            {{/alignCandidateSelect}}

            {{#alignSelectNow}}
            .alignSelectNow {
                fill: {{fill}};
                stroke: {{stroke}};
                background-color: {{background}};
            }
            {{/alignSelectNow}}

            {{#alignSelect}}
            .alignSelect {
                fill: {{fill}};
                stroke: {{stroke}};
                background-color: {{background}};
            }
            {{/alignSelect}}

            {{#align_Detail}}
            .align_Detail {
                stroke: {{stroke}};
            }
            {{/align_Detail}}
            {{#align_Detail_select}}
            .align_Detail_select {
                stroke: {{stroke}};
                fill : {{fill}};
            }
            {{/align_Detail_select}}

        {{/color}}
</style>